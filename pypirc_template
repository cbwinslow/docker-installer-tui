# This is a .pypirc template for uploading to PyPI
# Store this file in your home directory: ~/.pypirc

[distutils]
index-servers =
    pypi
    testpypi

[pypi]
repository: https://upload.pypi.org/legacy/
username: __token__
password: your-pypi-api-token-here

[testpypi]
repository: https://test.pypi.org/legacy/
username: __token__
password: your-testpypi-api-token-here